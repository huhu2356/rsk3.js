{
  "version": "0.2.0",
  "lerna": "2.0.0",
  "command": {
    "init": {
      "exact": true
    },
    "publish": {
      "exact": true,
      "skipGit": true
    },
    "version": {
      "allowBranch": [
        "master",
        "proj/release-*"
      ],
      "conventionalCommits": false,
      "exact": true,
      "message": "proj: release %s"
    }
  },
  "packages": [
    "packages/*"
  ]
}
